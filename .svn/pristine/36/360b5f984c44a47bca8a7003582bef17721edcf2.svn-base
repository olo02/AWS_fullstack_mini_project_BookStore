package team1.vo;

// 출판사 클래스
public class Publisher extends Member {
	// 사업자등록번호 10자리 "-" 제외
	private String brn;
	
	
	public String getBrn() {
		return brn;
	}

	public void setBrn(String brn) {
		this.brn = brn;
	}

	public Publisher() {
		super();
	}

	public Publisher(int code,String id, String name, String pw, String level, String brn) {
		super(code, id, pw, level, name);
		this.brn = brn;
	}
	@Override
	public String toString() {
		return "Publisher [brn=" + brn + "]";
	}
	
}
