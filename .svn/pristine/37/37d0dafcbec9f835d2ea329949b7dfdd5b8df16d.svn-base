package team1.vo;

// 출판사 클래스
public class Publisher extends Member {
	// 사업자등록번호 10자리 "-" 제외
	private int brn;
	
	
	public int getBrn() {
		return brn;
	}

	public void setBrn(int brn) {
		this.brn = brn;
	}

	public Publisher() {
		super();
	}

	public Publisher(String code, String id, String pw, String level, String name, int brn) {
		super(code, id, pw, level, name);
	}

	@Override
	public String toString() {
		return "Publisher [brn=" + brn + "]";
	}
	
}
