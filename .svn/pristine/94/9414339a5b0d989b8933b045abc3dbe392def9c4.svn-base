package team1.vo;

// 결제 클래스
public class Payment {
	private Book book;
	private Publisher publisher;
	
	public Payment() {
		super();
	}

	public Payment(Book book, Publisher publisher) {
		this.book = book;
		this.publisher = publisher;
	}

	public Book getBook() {
		return book;
	}

	public void setBook(Book book) {
		this.book = book;
	}

	public Publisher getPublisher() {
		return publisher;
	}

	public void setPublisher(Publisher publisher) {
		this.publisher = publisher;
	}
	
	
}
